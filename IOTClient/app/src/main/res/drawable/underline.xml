<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="#E6E6FA" />
            <solid android:color="#E6E6FA" />

        </shape>
    </item>

